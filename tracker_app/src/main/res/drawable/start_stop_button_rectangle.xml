<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="315dp"
        android:height="62dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/web_orange" />
            <corners android:radius="15dp" />
        </shape>
    </item>
    <item
        android:width="315dp"
        android:height="62dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <corners android:radius="15dp" />
            <stroke
                android:width="3dp"
                android:color="@color/web_orange" />
        </shape>

    </item>
</layer-list>